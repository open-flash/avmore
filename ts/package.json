{
  "name": "avmore",
  "version": "0.1.1",
  "homepage": "https://github.com/open-flash/avmore",
  "description": "Actionscript VM",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:open-flash/avm1-tree.git"
  },
  "licenses": [
    {
      "type": "AGPL-3.0-or-later",
      "url": "https://spdx.org/licenses/AGPL-3.0-or-later.html"
    }
  ],
  "keywords": [
    "swf",
    "ast",
    "avm1"
  ],
  "scripts": {
    "build": "gulp lib:build",
    "watch": "gulp lib:watch",
    "test": "gulp test",
    "lint": "gulp lint",
    "format": "gulp format",
    "typedoc": "gulp lib:typedoc",
    "dist": "gulp lib:dist",
    "prepare": "gulp all:tsconfig.json && gulp tslint.json && gulp dist",
    "pretest": "gulp lint",
    "prepublishOnly": "echo \"Use \\`gulp lib:publish\\`\" && exit 1",
    "prestart": "gulp main:build",
    "start": "node build/main/main/main.js"
  },
  "dependencies": {
    "avm1-parser": "^0.9.1",
    "avm1-types": "^0.9.0",
    "incident": "^3.2.0",
    "semantic-types": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/gulp": "^4.0.6",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.8",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "minimist": "^1.2.6",
    "swf-parser": "^0.8.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "turbo-gulp": "^0.20.1",
    "typescript": "^3.6.3"
  },
  "c88": {
    "match": [
      "build/test/lib/*.{js,mjs}",
      "build/test/lib/**/*.{js,mjs}"
    ]
  }
}
